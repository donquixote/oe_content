{
    "name": "openeuropa/oe_content",
    "description": "OpenEuropa Drupal module that contains the EC corporate entity types.",
    "type": "drupal-module",
    "license": "EUPL-1.2",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "drupal/core": "^8.8",
        "drupal/linkit": "~5.0-beta8",
        "drupal/maxlength": "~1.0@beta",
        "easyrdf/easyrdf": "0.10.0-alpha.1 as 0.9.1",
        "openeuropa/rdf_skos": "~0.8",
        "openeuropa/oe_media": "^1.4",
        "php": ">=7.2"
    },
    "require-dev": {
        "composer/installers": "~1.5",
        "drupal-composer/drupal-scaffold": "~2.5.2",
        "drupal/address": "~1.7",
        "drupal/config_devel": "~1.2",
        "drupal/datetime_testing": "1.x-dev",
        "drupal/drupal-extension": "~4.0",
        "drupal/entity_embed": "~1.0",
        "drupal/entity_reference_revisions": "~1.3",
        "drupal/field_group": "~3.0",
        "drupal/inline_entity_form": "~1.0-rc3",
        "drupal/typed_link": "~1.1",
        "drupaltest/behat-traits": "dev-8.x-1.x",
        "drush/drush": "~9.0",
        "friends-of-behat/service-container-extension": "^1.0",
        "guzzlehttp/guzzle": "~6.3",
        "nikic/php-parser": "^3.1.5",
        "openeuropa/behat-transformation-context": "~0.1",
        "openeuropa/code-review": "~1.3.0",
        "openeuropa/drupal-core-require-dev": "^8.7",
        "openeuropa/oe_corporate_countries": "~1.0.0-beta1",
        "openeuropa/oe_link_lists": "~0.5.0",
        "openeuropa/oe_multilingual": "dev-master",
        "openeuropa/oe_time_caching": "^1.0.0",
        "openeuropa/task-runner": "~1.0@beta",
        "phpunit/phpunit": "~6.0",
        "symfony/dom-crawler": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\oe_content\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\oe_content\\": "./tests/",
            "Drupal\\Tests\\oe_content_featured_media_field\\": "./modules/oe_content_featured_media_field/tests/src/",
            "Drupal\\Tests\\oe_media\\": "./build/modules/contrib/oe_media/tests/"
        }
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": "./vendor/bin/run drupal:site-setup",
        "post-update-cmd": "./vendor/bin/run drupal:site-setup"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "extra": {
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "installer-paths": {
            "build/core": ["type:drupal-core"],
            "build/profiles/contrib/{$name}": ["type:drupal-profile"],
            "build/modules/contrib/{$name}": ["type:drupal-module"],
            "build/themes/contrib/{$name}": ["type:drupal-theme"]
        },
        "patches": {
            "drupal/field_group": {
                "https://www.drupal.org/project/field_group/issues/2787179#comment-13467953": "https://www.drupal.org/files/issues/2020-02-17/2787179-highlight-html5-validation-45.patch"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
